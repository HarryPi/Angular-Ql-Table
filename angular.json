{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ql-table": {
      "projectType": "library",
      "root": "projects/ql-table",
      "sourceRoot": "projects/ql-table/src",
      "prefix": "ql",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "tsConfig": "projects/ql-table/tsconfig.lib.json",
            "project": "projects/ql-table/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/ql-table/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/ql-table/src/test.ts",
            "tsConfig": "projects/ql-table/tsconfig.spec.json",
            "karmaConfig": "projects/ql-table/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/ql-table/tsconfig.lib.json",
              "projects/ql-table/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false
  },
  "defaultProject": "ql-table",
  "schematics": {
    "@schematics/angular:component": {
      "styleext": "scss"
    }
  }
}