{
  "extends": "../../tslint.json",
  "rules": {
    "variable-name": {
      "options": [
        "ban-keywords",
        "check-format",
        "allow-pascal-case",
        "allow-leading-underscore"
      ]
    },
    "directive-selector": [
      true,
      "attribute",
      "ql",
      "camelCase"
    ],
    "no-host-metadata-property": [
      false
    ],
    "component-selector": [
      true,
      "element",
      "ql",
      "kebab-case"
    ]
  }
}
